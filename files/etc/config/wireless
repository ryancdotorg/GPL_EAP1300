	option wps_ext_registrar '0'
	option wps_state '1'
	option wps_disabled_pin '0'
	option wps_device_name 'Engenius Dual Radio Wall Plate AP'
	option wps_manufacturer 'Senao Technologies, Inc.'
	option wps_friendly_name 'Dual Radio Wall Plate AP'
	option wps_manufacturer_url 'http://www.engeniustech.com/'
	option wps_model_name 'Engenius Dual Radio Wall Plate AP'
	option wps_model_number 'EAP1300'
	option wps_model_description 'Dual Radio Wall Plate AP'
	option wps_model_url 'http://www.engeniustech.com/'
	option wpa_group_rekey '3600'
	option encrDisabled '0'
	option encryptionFake 'psk2+ccmp'
	option wps_pin '12345678'
	option acct_enabled '0'
	option nasid_enable '0'
	option nasid	''
	option nasip_enable '0'
	option nasip	''
	option nasport_enable '0'
	option bandsteer_en '0'
	option guest_network 'Disable'

