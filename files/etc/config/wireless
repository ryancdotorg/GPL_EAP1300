config wifi-device 'wifi1'
	option type 'qcawifi'
	option channel 'auto'
	option phy 'wifi1'
	option hwmode '11ac'
	option txpower '0'
	option bandsteer '0'
	option fasthandover_status '0'
	option fasthandover_rssi '-85'
	option htmode 'HT40'
	option opmode 'ap'
	option disabled '0'
	option country '00'
	option clientlimits_enable '1'
	option clientlimits_number '127'
	option aggregation_enable '1'
	option aggregation_frame '32'
	option aggregation_byte '50000'
	option obeyregpower 'undefined'
	option frag '2346'
	option rts '2346'
	option bintval '100'
	option preamble '0'
	option dtim_period '2'
	option bandsteerpersent '75'
	option bandsteerrssi '20'
	option bandsteerHBpersent '50'
	option bandsteerHBrssi '30'
	option distance '1000'
	option ext_channel '0'
	option txchainmask '3'
	option nochannel '0'
	option min_rate '6'
	option channel_config_enable '0'
	option qwrap_enable '0'
	option qwrap_eth_sta_add_en '0'
        option backgroundscanEnable '0'
        option switchChanInterval '20'
        option mindwell '51'
        option maxdwell '100'
        option resttime '1'
        option maxscantime '200'
        option idletime '51'

