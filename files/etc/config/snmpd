	option 'level' 'noauth'
	option 'prefix' 'exact'
	option 'read' 'all'
	option 'write' 'none'
	option 'notify' 'none'

config 'access' 'private_access'
	option 'group' 'private'
	option 'context' 'none'
	option 'version' 'any'
	option 'level' 'noauth'
	option 'prefix' 'exact'
	option 'read' 'all'
	option 'write' 'all'
	option 'notify' 'all'

