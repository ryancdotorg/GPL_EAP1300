#!/bin/sh

uci -q batch <<-EOF >/dev/null
	del ucitrack.@fingerprint[-1]
	del_list ucitrack.@network[-1].affects=fingerprint
	del_list ucitrack.@system[-1].affects=fingerprint
	add ucitrack fingerprint
	set ucitrack.@fingerprint[0].init=fingerprint
	add_list ucitrack.@network[0].affects=fingerprint
	add_list ucitrack.@system[0].affects=fingerprint
	commit ucitrack
EOF

rm -f /tmp/luci-indexcache
exit 0
